<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <TextView
        android:fontFamily="@font/suit_extra_bold_800"
        android:text="폰트 테스트 suit_extra_bold"
        android:textSize="32sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:fontFamily="@font/suit_bold_700"
        android:text="폰트 테스트 suit_bold"
        android:textSize="32sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:fontFamily="@font/suit_semi_bold_600"
        android:text="폰트 테스트 suit_semi_bold"
        android:textSize="32sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:text="폰트 테스트 suit_medium"
        android:fontFamily="@font/suit_medium_400"
        android:layout_width="match_parent"
        android:textSize="32sp"

        android:layout_height="wrap_content"/>


    <TextView
        android:text="폰트 테스트 normal_font 기본"
        android:layout_width="match_parent"
        android:textSize="32sp"
        android:layout_height="wrap_content"/>
</LinearLayout>